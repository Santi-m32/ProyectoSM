package Paquete;
public class Vehiculos {

	String Marca;
	String Precio;
	}
	
	class Autos extends Vehiculos
	{
		int ModeloAu;
		int Puerta;

		public Autos()
		{
			this.Marca="Peugeot";
		}
		public String Marca()
		{
			return this.Marca();
		}
	}
	class Motos extends Vehiculos
	{
		String ModeloMot;
		String Cilindrada;
	}