package Paquete;
public class Vehiculos {

	String Marca;
	String Precio;
	}
	
	class Autos extends Vehiculos
	{
		int Modelo;
		int Puerta;
		public Autos()
		{
			this.Modelo=0;
			this.Puerta=0;
		}
		public void MarcaA()
		{
			System.out.println("Peugeot");
		}
	}
	class Motos extends Vehiculos
	{
		String Modelo;
		String Cilindrada;
	}
