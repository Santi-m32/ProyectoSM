package paquete;
import java.io.*;
class main {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		char text;
		InputStreamReader in = new InputStreamReader(System.in);
		BufferedReader buffer= new BufferedReader(in);
		try{
			text = buffer.read();
			}catch(IOException e){};
			System.out.println(text);
			
		}

}
